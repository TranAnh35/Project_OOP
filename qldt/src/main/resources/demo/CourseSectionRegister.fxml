<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox alignment="CENTER_LEFT" prefHeight="40.0" prefWidth="875.0" stylesheets="@Design/DashBoardDesign.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="demo.CourseSectionController">
   <children>
      <Label fx:id="CourseName" alignment="CENTER" prefHeight="21.0" prefWidth="373.0" text="Course Section Name">
         <font>
            <Font size="15.0" />
         </font>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </Label>
      <Label fx:id="LecturerName" alignment="CENTER" prefHeight="21.0" prefWidth="256.0" text="Main Lecturer">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Label fx:id="Enrolled" alignment="CENTER" prefHeight="21.0" prefWidth="124.0" text="Enrolled">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Button fx:id="register_btn" alignment="CENTER" mnemonicParsing="false" onAction="#registerCousreSection" styleClass="update-btn" text="Register">
         <HBox.margin>
            <Insets left="25.0" />
         </HBox.margin>
      </Button>
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</HBox>
