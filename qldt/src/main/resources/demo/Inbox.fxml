<?xml version="1.0" encoding="UTF-8"?>

<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIcon?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox alignment="CENTER_LEFT" prefHeight="40.0" prefWidth="718.0" stylesheets="@Design/DashBoardDesign.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="demo.InboxController">
   <children>
      <CheckBox fx:id="check_box" mnemonicParsing="false" visible="false">
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <HBox.margin>
            <Insets left="10.0" />
         </HBox.margin>
      </CheckBox>
      <Label fx:id="sender_name" alignment="CENTER" prefHeight="17.0" prefWidth="205.0" text="Sender Name">
         <HBox.margin>
            <Insets left="10.0" />
         </HBox.margin>
         <font>
            <Font size="18.0" />
         </font>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </Label>
      <Label fx:id="tiltle_inbox" alignment="CENTER" prefHeight="27.0" prefWidth="442.0" text="Tiltle of inbox">
         <font>
            <Font size="18.0" />
         </font>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </Label>
      <Button fx:id="Inbox_read" mnemonicParsing="false" styleClass="add-btn">
         <graphic>
            <FontAwesomeIcon fill="WHITE" glyphName="EYE" />
         </graphic>
      </Button>
   </children>
</HBox>
